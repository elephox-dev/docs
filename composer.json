{
    "name": "elephox/docs",
    "description": "Elephox Documentation",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 <8.3",
        "ext-fileinfo": "*",
        "elephox/framework": "^0.4",
        "nunomaduro/collision": "^6.2",
        "erusev/parsedown-extra": "^0.8",
        "filp/whoops": "^2.14",
        "keinos/parsedown-toc": "^1.1",
        "scrivo/highlight.php": "9.18.1.9",
        "elephox/plane": "^2.0",
        "elephox/builder-whoops-handler": "^0.1.0",
        "elephox/builder-whoops-middleware": "^0.1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Elephox\\Docs\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env.local') || copy('.env.local.example', '.env.local');\""
        ]
    }
}
